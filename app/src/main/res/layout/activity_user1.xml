<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/texture5"
    tools:context="com.example.nimal.help.User1">


    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="73dp"
        android:clickable="true"
        android:onClick="helping"
        android:layout_marginTop="104dp"
        app:srcCompat="@drawable/image8" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/images8"
        android:clickable="true"
        android:onClick="location"
        android:layout_marginBottom="75dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageView15" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView15"
        android:text="Help"
        android:onClick="helping"
        android:clickable="true"
        android:gravity="center"
        android:textStyle="bold"
        android:layout_alignBottom="@+id/imageView15"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView15" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView17"
        android:layout_alignParentEnd="true"
        android:text="Location"
        android:gravity="center"
        android:clickable="true"
        android:onClick="location"
        android:textStyle="bold"
        android:layout_toEndOf="@+id/imageView17"
        android:layout_alignTop="@+id/imageView17" />
</RelativeLayout>
